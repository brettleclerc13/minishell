void	g_var_value(char *char_dol)
{
	*char_dol = char_dol[1];
	printf("minishell: %d%s: command not found\n", g_var, char_dol);
	g_var = 127;
}

void	one_dollar(void)
{
	ft_putstr_fd("minishell: $: command not found\n", STDOUT_FILENO);
	g_var = 127;
	return ;
}

